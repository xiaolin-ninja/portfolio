{
  "name": "playground-macos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.ts,.tsx ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --ext .js,.ts,.tsx ./"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "none"
        }
      ]
    },
    "ignorePatterns": [
      "node_modules/",
      "build/"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@rooks/use-raf": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "date-fns": "2.23.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "framer-motion": "^4.1.17",
    "nightwind": "^1.1.12",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-markdown": "^7.0.0",
    "react-rangeslider": "2.2.0",
    "react-redux": "^7.2.4",
    "react-rnd": "^10.3.4",
    "react-scripts": "4.0.3",
    "react-syntax-highlighter": "^15.4.4",
    "react-webcam": "6.0.0",
    "redux": "^4.1.1",
    "remark-gfm": "^2.0.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "postcss": "^7",
    "sort-package-json": "^1.50.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.3.5"
  }
}
